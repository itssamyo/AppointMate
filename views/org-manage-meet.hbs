
    <div class="row">
        <div class="col"><br>
            <h5 style="text-align:center"><b>INPUT INFORMAITON</b></h5>
             <br><h6>ATTENDEES</h6>
        </div>
    </div>   
    <div class="row" style=" height: auto; box-shadow: 0px 0px 15px -2px;border-radius:3px;">
        <div class="col">
            <table class="table table-hover">
                <thead>
                <tr>
                    <th>Firstname</th>
                    <th>Lastname</th>
                    <th>Email</th>
                </tr>
                </thead>
                <tbody>
                {{# each attendee as |res| }}
                    <tr>
                        <td>{{ res.aFname }}</td>    
                        <td>{{ res.aLname }}</td>                
                        <td>{{ res.aEmail }}</td>
                    </tr>
                {{/each}}
            </table>
        </div>
    </div><br><br>
    <h6>SLOTS</h6>
    <div class="row" style=" height: auto; box-shadow: 0px 0px 15px -2px;border-radius:3px;">
        <div class="col">
            <table class="table table-hover">
                <thead>
                <tr>
                    <th>Start Time</th>
                    <th>End time</th>
                </tr>
                </thead>
                <tbody>
                {{# each slot as |res| }}
                    <tr>
                        <td>{{ res.sStart }}</td>    
                        <td>{{ res.sEnd }}</td>                
                    </tr>
                {{/each}}
            </table>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div style="float:right">
            <br><a href="/users/org/confirm-meet" style="color:white" class="btn btn-success">Confirm Meeting</a>
            </div>
        </div>
    </div>
